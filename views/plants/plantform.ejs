<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" defer></script>
<form action="/submit" class="form-group" method="POST">
    <textarea name="message" id="" cols="30" rows="10" placeholder="Textarea placeholder"></textarea>
    <button class="btn btn-primary" type="submit">Submit</button>
</form>

<form action="">
    <label for="inputlabel">Input placeholder</label>
    <input type="text" id="inputlabel">
    <button class="btn btn-success">Submit</button>
</form>

<div class="container">
    <h1 class="mb-4">Plant Articles</h1>
    <a href="/articles/new" class="btn btn-success">New Article</a>
    <div class="card-body">
        <h4 class="card-title"><% article.title %></h4>
        <div class="card-subtitle text-muted mb-2">
            <%= article.createdAt.toLocaleDateString() %>
        </div>
        <div class="class-text mb-2"><%= article.description %></div>
    </div>
</div>

